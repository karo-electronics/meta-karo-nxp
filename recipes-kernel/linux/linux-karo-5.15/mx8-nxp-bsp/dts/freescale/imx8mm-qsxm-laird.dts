/dts-v1/;

/plugin/;

&{/chosen} {
	overlays {
		qsxm-laird;
	};
};

&pcie0 {
	#address-cells = <3>;
	#size-cells = <2>;
	status = "okay";

	wifi@0,0 {
		compatible = "pci1b4b,2b42";
		reg = <0 0 0 0 0>;
		status = "okay";
	};
};

&uart1 {
	/* BT UART */
	status = "okay";
};
