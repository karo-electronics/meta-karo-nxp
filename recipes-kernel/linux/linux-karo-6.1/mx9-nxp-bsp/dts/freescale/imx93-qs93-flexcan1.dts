/dts-v1/;

/plugin/;

#include "imx93-pinfunc.h"

&{/chosen} {
	overlays {
		qs93-flexcan1;
	};
};

&flexcan1 {
	xceiver-supply = <&reg_vdd_3v3>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan1>;
	status = "okay";
};

&iomuxc {
	pinctrl_flexcan1: flexcan1grp {
		fsl,pins = <
			MX93_PAD_PDM_BIT_STREAM0__CAN1_RX	0x139e
			MX93_PAD_PDM_CLK__CAN1_TX		0x139e
		>;
	};
};
