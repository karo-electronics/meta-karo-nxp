/dts-v1/;

/plugin/;

&{/chosen} {
	overlays {
		karo-panel-tm101jvhg32;
	};
};

#define PIXCLK 68200000

&lcdif {
	assigned-clock-rates = <(PIXCLK * 7)>, <PIXCLK>, <400000000>, <133333333>;
};

&panel {
	label = "TM101JVHG32";
	data-mapping = "vesa-24";
	width-mm = <217>;
	height-mm = <136>;
	status = "okay";

	panel-timing {
		clock-frequency = <PIXCLK>;
		hactive = <1280>;
		vactive = <800>;
		hback-porch = <6>;
		hsync-len = <2>;
		hfront-porch = <15>;
		vback-porch = <2>;
		vsync-len = <1>;
		vfront-porch = <3>;
		hsync-active = <0>;
		vsync-active = <0>;
		de-active = <1>;
		pixelclk-active = <0>;
	};
};
