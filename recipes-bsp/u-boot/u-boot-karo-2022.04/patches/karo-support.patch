--- /dev/null
+++ u-boot-tx93-mb7/arch/arm/dts/imx93-qs93-5210-u-boot.dtsi
@@ -0,0 +1,317 @@
+// SPDX-License-Identifier: (GPL-2.0 or MIT)
+/*
+ * Copyright 2023 Lothar Waßmann <LW@KARO-electronics.de>
+ */
+
+/ {
+	aliases {
+		usbgadget0 = &usbg1;
+		usbgadget1 = &usbg2;
+	};
+
+#ifdef CONFIG_BINMAN
+	binman: binman {
+		multiple-images;
+	};
+#endif
+	chosen {
+		u-boot,dm-spl;
+		u-boot,dm-pre-reloc;
+	};
+
+	usbg1: usbg1 {
